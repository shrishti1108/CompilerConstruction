%{
	
	#include<stdio.h>
	
	char convert_case(char c)
	{
		if (c >= 'a' && c<= 'z')
		{
			return c - 'a' + 'A';
		}
		else if(c >= 'A' && c<= 'Z')
		{
			return c - 'A' + 'a';
		}
		return c;
	}
%}

